{
  "version": 2,
  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": ["/random-game", "/games"],
      "dest": "app.js"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}
